#: forms.py:11
msgid "Phone number must be entered in the format: '+999999999'. Up to 15 digits allowed."
msgstr "Số điện thoại phải có định dạng sau: '+999999999'. Độ dài tối đa là 15 kí tự."

#: forms.py:12
msgid "Phone number"
msgstr "Số điện thoại"

#: forms.py:13
msgid "Address"
msgstr "Địa chỉ"

#: i18n.py:10
msgid "Enter the same password as before, for verification."
msgstr "Nhập lại mật khẩu trên để xác minh."

#: i18n.py:11
msgid "Required. 150 characters or fewer. Letters, digits and @/./+/-/_ only."
msgstr "Bắt buộc. Tối đa 150 kí tự. Chỉ được nhập số, chữ cái và @/./+/-/_"

#: i18n.py:12
#, python-format
msgid "The password is too similar to the %(verbose_name)s."
msgstr "Mật khẩu có chứa thông tin giống với %(verbose_name)s."

#: i18n.py:13
msgid "Your password can’t be too similar to your other personal information."
msgstr "Mật khẩu không được chứa thông tin cá nhân của bạn."

#: i18n.py:14
msgid "This password is too common."
msgstr "Mật khẩu này quá phổ biến."

#: i18n.py:15
msgid "Your password can’t be a commonly used password."
msgstr "Không được đặt các mật khẩu quá phổ biến."

#: i18n.py:16
msgid "This password is entirely numeric."
msgstr "Mật khẩu này chỉ chứa toàn số."

#: i18n.py:17
msgid "Your password can’t be entirely numeric."
msgstr "Không được đặt mật khẩu chỉ chứa toàn số."

#: i18n.py:18
msgid "Enter a valid username. This value may contain only letters, numbers, and @/./+/-/_ characters."
msgstr "Lưu ý: Tên đăng nhập hợp lệ chỉ được chứa số, chữ cái hoặc các kí tự @/./+/-/_"

#: i18n.py:20
#, python-format
msgid "Your password must contain at least %(min_length)d character."
msgid_plural "Your password must contain at least %(min_length)d characters."
msgstr[0] "Mật khẩu của bạn phải chứa ít nhất %(min_length)d kí tự."
msgstr[1] "Mật khẩu của bạn phải chứa ít nhất %(min_length)d kí tự."

#: i18n.py:25
#, python-format
msgid "This password is too short. It must contain at least %(min_length)d character."
msgid_plural "This password is too short. It must contain at least %(min_length)d characters."
msgstr[0] "Mật khẩu này quá ngắn. Mật khẩu phải chứa ít nhất %(min_length)d kí tự."
msgstr[1] "Mật khẩu này quá ngắn. Mật khẩu phải chứa ít nhất %(min_length)d kí tự."

#: i18n.py:29
msgid "The two password fields didn’t match."
msgstr "Hai trường mật khẩu không giống nhau."

